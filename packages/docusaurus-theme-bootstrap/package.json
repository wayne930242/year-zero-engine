{
  "name": "@docusaurus/theme-bootstrap",
  "version": "2.0.0-alpha.66",
  "description": "Bootstrap theme for docusaurus",
  "main": "src/index.js",
  "types": "src/types.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
